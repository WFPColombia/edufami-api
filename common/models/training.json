{
  "name": "Training",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "created": {
      "type": "date"
    },
    "edited": {
      "type": "date"
    },
    "active": {
      "type": "boolean"
    },
    "app": {
      "type": "string"
    },
    "status": {
      "type": "number"
    },
    "image": {
      "type": "string"
    },
    "languageId": {
      "type": "string"
    },
    "ownerId": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "UserAccount",
      "foreignKey": "ownerId"
    },
    "units": {
      "type": "hasMany",
      "model": "Unit",
      "foreignKey": "trainingId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
